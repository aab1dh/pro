{
  "name": "pro",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/pro/pro.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "bundledDependencies": [
    "utils"
  ],
  "scripts": {
    "build": "yarn prettify && yarn lint && yarn test && stencil build --docs",
    "prerender": "yarn prettify && yarn lint && yarn test && stencil build --docs --dev --prerender",
    "start:dev": "yarn prettify && yarn lint && yarn test && stencil build --dev --watch --serve",
    "start": "yarn prettify && yarn lint && yarn test && stencil build --watch --serve",
    "generate": "stencil generate",
    "test": "stencil test --spec --e2e --json --outputFile=.jest-test-results.json || true",
    "test.watch": "stencil test --spec --watchAll",
    "test.end-to-end": "stencil test --e2e",
    "storybook": "yarn build && start-storybook -p 6006 -s ./src",
    "build-storybook": "build-storybook -s ./src/assets:assets -c ./.storybook -o ./storybook-static",
    "storybook-server": "http-server-spa storybook-static/ index.html 6006",
    "lint": "eslint src/**/*{.ts,.tsx} --fix",
    "prettify": "prettier --config ./.prettierrc.json 'src/**/*.@(js|jsx|ts|tsx|mdx)' --write",
    "chromatic": "npx chromatic --project-token <CHROMATIC_PROJECT_TOKEN>",
    "sonar": "sonar-scanner   -D sonar.projectKey=pro-framework   -D sonar.sources=.   -D sonar.host.url=http://localhost:9000   -D sonar.login=aeed4ab9d33da44279d6baab58dbc4611c5df5ed"
  },
  "dependencies": {
    "@stencil/core": "^2.14.2"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.8",
    "@hover/storybook-addon-pseudo-states": "^1.1.3",
    "@stencil/eslint-plugin": "^0.4.0",
    "@stencil/helmet": "^0.3.2",
    "@stencil/sass": "^1.5.2",
    "@stencil/store": "^1.5.0",
    "@storybook/addon-a11y": "^6.4.21",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-controls": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-interactions": "^6.4.21",
    "@storybook/addon-jest": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/addon-measure": "^6.4.21",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-storysource": "^6.4.21",
    "@storybook/addons": "^6.4.21",
    "@storybook/html": "^6.4.21",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/testing-library": "^0.0.9",
    "@storybook/theming": "^6.4.21",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@washingtonpost/storybook-addon-web-vitals": "^0.3.0",
    "babel-loader": "^8.2.3",
    "case": "^1.6.3",
    "chromatic": "^6.5.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-storybook": "^0.5.8",
    "jest": "^27.5.1",
