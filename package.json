{
  "name": "pro",
  "version": "1.0.0",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/pro/pro.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "private": true,
  "postinstall": "cd apps/app && yarn",
  "scripts": {
    "turbo:build": "turbo run build",
    "turbo:prerender": "turbo run prerender",
    "turbo:start": "turbo run start",
    "turbo:storybook": "turbo run storybook",
    "turbo:build-storybook": "turbo run build-storybook",
    "prerender": "cd apps/app && yarn prerender",
    "build": "cd apps/app && yarn build",
    "start": "cd apps/app && yarn start",
    "storybook": "cd apps/app && yarn storybook",
    "chromatic": "cd apps/app && yarn chromatic",
    "sonar": "cd apps/app && yarn sonar",
    "test": "cd apps/app && yarn test",
    "build-storybook": "cd apps/app && yarn build-storybook"
  },
  "dependencies": {},
  "license": "MIT",
  "devDependencies": {
    "turbo": "^1.2.4"
  }
}
